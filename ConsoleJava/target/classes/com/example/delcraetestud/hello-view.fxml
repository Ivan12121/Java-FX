<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="504.0" prefWidth="406.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.delcraetestud.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane prefHeight="27.0" prefWidth="284.0">
      <children>
         <Label alignment="CENTER" layoutX="27.0" layoutY="5.0" prefHeight="20.0" prefWidth="320.0" text="Добавление и удаление студентов" textAlignment="CENTER">
            <font>
               <Font size="14.0" />
            </font>
         </Label>
      </children>
   </AnchorPane>
   <AnchorPane prefHeight="306.0" prefWidth="284.0">
      <children>
      
          <Label fx:id="welcomeText" layoutX="76.0" layoutY="276.0" prefHeight="25.0" prefWidth="214.0" />
          <Button layoutX="152.0" layoutY="301.0" onAction="#onHelloButtonClick" text="Помощь" />
         <Button layoutX="124.0" layoutY="138.0" mnemonicParsing="false" onAction="#openCreateWindow" text="Добавить студента" />
         <Button layoutX="123.0" layoutY="183.0" mnemonicParsing="false" onAction="#openDeleteWindow" prefHeight="25.0" prefWidth="120.0" text="Удалить студента" />
         <Button layoutX="123.0" layoutY="225.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="120.0" text="Найти по имени" />
      </children>
   </AnchorPane>
</VBox>
